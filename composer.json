{
    "name": "reker7/moonshine-blocks-core",
    "description": "Core models and migrations for MoonShine Blocks",
    "keywords": ["moonshine", "blocks", "core", "models"],
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2|^8.4",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Reker7\\MoonShineBlocksCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reker7\\MoonShineBlocksCore\\Tests\\": "tests/",
            "Reker7\\MoonShineFieldsBuilder\\": "../moonshine-fields-builder/src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reker7\\MoonShineBlocksCore\\Providers\\BlocksCoreServiceProvider"
            ]
        }
    }
}
